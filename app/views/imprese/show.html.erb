  <dd><%= @impresa.lat %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lng) %>:</strong></dt>
  <dd><%= @impresa.lng %></dd>
  <dt><strong><%= model_class.human_attribute_name(:verificato) %>:</strong></dt>
  <dd><%= @impresa.verificato %></dd>
  <dt><strong><%= model_class.human_attribute_name(:congelato) %>:</strong></dt>
  <dd><%= @impresa.congelato %></dd>
  <dt><strong><%= model_class.human_attribute_name(:citta) %>:</strong></dt>
  <dd><%= @impresa.getCitta %></dd>
  <dt><strong><%= model_class.human_attribute_name(:titolare) %>:</strong></dt>
  <dd><%= @impresa.getTitolare %></dd>
</dl>
<%= link_to "I Nostri Prodotti", impresa_prodotti_path(impresa_nome: @impresa.nome,id: params[:id]), :class => 'btn btn-default' ,method: :get %>

<%= link_to t('.tutte_le_imprese', :default => t("helpers.links.tutte_le_imprese")),
              imprese_path, :class => 'btn btn-default' ,method: :get %>
