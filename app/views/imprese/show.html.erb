,method: :get<%- model_class = Impresa -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @impresa.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefono) %>:</strong></dt>
  <dd><%= @impresa.telefono %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fax) %>:</strong></dt>
  <dd><%= @impresa.fax %></dd>
  <dt><strong><%= model_class.human_attribute_name(:giorni_orari) %>:</strong></dt>
  <dd><%= @impresa.giorni_orari %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @impresa.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sitoweb) %>:</strong></dt>
  <dd><%= @impresa.sitoweb %></dd>
  <dt><strong><%= model_class.human_attribute_name(:facebook) %>:</strong></dt>
  <dd><%= @impresa.facebook %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descrizione) %>:</strong></dt>
  <dd><%= @impresa.descrizione %></dd>
